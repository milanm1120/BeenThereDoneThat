<h1> Welcome Traveller, Lets Get You Registered! </h1>

<%= render partial: "layouts/error", locals: { object: @user} %>

<%= form_with(model: @user) do |f| %>
    First Name: <%= f.text_field :first_name%></br>
    Last Name: <%= f.text_field :last_name%></br>
    Email: <%= f.text_field :email %></br>
    Password: <%= f.password_field :password %></br></br>
    <%= f.submit "Register" %>
<% end %>
<br>
<%= button_to 'Back To Welcome Page', root_path, :method => :get %>